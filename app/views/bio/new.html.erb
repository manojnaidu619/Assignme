<%= form_for @bio, url: {action: 'create'} do |f| %>
 <%= f.label 'Full Name:' %>
 <%= f.text_field :fullname %><br>
 <%= f.label 'Username' %>
 <%= f.text_field :username %><br>
 <%= f.label 'Locality' %>
 <%= f.text_field :locality %><br>
 <%= f.label 'Occupation' %>
 <%= f.select :occupation, choose: Bio.occupations.keys %><br>
 <%= f.label "Contact Number" %>
 <%= f.number_field :contact_number %><br>
 <%= f.label 'school/college Name' %>
 <%= f.text_field :college_school_name %><br>
 <%= f.label 'DOB' %>
 <%= f.date_field :dob %><br>
 <%= f.submit %>
<% end %>
